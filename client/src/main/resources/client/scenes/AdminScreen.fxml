<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" styleClass="border_plane" stylesheets="@css/LobbyCSS.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Region minWidth="30.0" prefWidth="100.0" />
            <VBox prefHeight="128.0" prefWidth="200.0">
               <children>
                  <Region VBox.vgrow="ALWAYS" />
                  <Button fx:id="backbutton" mnemonicParsing="false" onAction="#onBackClick" prefHeight="90.0" prefWidth="200.0" text="Back">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </Button>
                  <Region VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <Region HBox.hgrow="ALWAYS" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title" text="Admin Interface" textAlignment="CENTER" wrappingWidth="696.4375">
                    <font>
                        <Font size="96.0" />
                    </font>
                </Text>
            <Region prefHeight="128.0" HBox.hgrow="ALWAYS" />
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Region VBox.vgrow="ALWAYS" />
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddClick" prefHeight="67.0" prefWidth="200.0" text="Add">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </Button>
                  <Region VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
                <Region minWidth="30.0" prefWidth="100.0" />
            </children>
        </HBox>
    </top>
    <center>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <Region minHeight="30.0" prefHeight="0.0" prefWidth="1920.0" />
                <HBox maxHeight="1006.0" prefHeight="598.0" prefWidth="1920.0">
                    <children>
                        <Region HBox.hgrow="ALWAYS" />
                        <ListView fx:id="activitiesListView" onMouseClicked="#onClick" prefHeight="638.0" prefWidth="808.0" />
                        <Region HBox.hgrow="ALWAYS" />
                    </children>
                </HBox>
                <Region VBox.vgrow="ALWAYS" />
            </children>
        </VBox>
    </center>
</BorderPane>
