<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" styleClass="border_plane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.QuitScreenCtrl">
    <center>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <Region minHeight="0.0" prefWidth="1920.0" VBox.vgrow="ALWAYS" />
                <HBox maxHeight="616.0" prefHeight="188.0" prefWidth="1920.0">
                    <children>
                        <Region HBox.hgrow="ALWAYS" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title" text="Are you sure you want to Quit? ">
                     <font>
                        <Font size="61.0" />
                     </font>
                  </Text>
                        <Region HBox.hgrow="ALWAYS" />
                    </children>
                </HBox>
                <Region minHeight="30.0" prefHeight="50.0" />
                <HBox maxHeight="616.0" prefHeight="110.0" prefWidth="1920.0">
                    <children>
                        <Region HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" onAction="#goBackToMain" prefHeight="150.0" prefWidth="500.0" text="No">
                            <font>
                                <Font size="57.0" />
                            </font>
                        </Button>
                        <Region minWidth="100.0" prefHeight="122.0" prefWidth="240.0" />
                  <Button mnemonicParsing="false" onAction="#quitGame" prefHeight="150.0" prefWidth="500.0" text="Yes">
                     <font>
                        <Font size="57.0" />
                     </font>
                  </Button>
                  <Region HBox.hgrow="ALWAYS" />
                    </children>
                </HBox>
            <Region prefHeight="30.0" VBox.vgrow="ALWAYS" />
            </children>
        </VBox>
    </center>
</BorderPane>
